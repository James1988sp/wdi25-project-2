<% beaches.forEach((beach) => { %>
  <div>
    <a href="/beaches/<%= beach.id %>">
      <img src="<%= beach.src %>" alt="<%= beach.name %>">
      <ul>
      Name : <h2><%= beach.name %></h2></a>
      Country : <h3><%= beach.country %></h3>
      Seabed : <h3><%= beach.seabed %></h3>
      Waves : <h3><%= beach.waves %></h3><br>
      <p><%= beach.description %></p>
      </ul>
  </div>
<% }) %>
